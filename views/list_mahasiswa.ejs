<!DOCTYPE html>
<html>
  <head>
    <title>List Mahasiswa</title>
  </head>
  <body>
    <h1>List Mahasiswa</h1>
    <ul>
      <% for (var i = 0; i < mahasiswa.length; i++) { %>
      <li><%= mahasiswa[i].nama %> - <%= mahasiswa[i].nim %></li>
      <% } %>
    </ul>
    <a href="/">Kembali ke Beranda</a>
  </body>
</html>
