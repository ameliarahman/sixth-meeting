<!DOCTYPE html>
<html>

<head>
    <title>List Mahasiswa</title>
</head>

<body>
    <h1>List Mahasiswa</h1>
    <ul>
        <% mahasiswa.forEach(mhs => { %>
            <li>
                <p>Id:
                    <%= mhs.id %>
                </p>
                <p>Nama:
                    <%= mhs.name %>
                </p>
                <p>NIM:
                    <%= mhs.nim %>
                </p>
                <p>Alamat:
                    <%= mhs.address %>
                </p>
            </li>
            <% }) %>
    </ul>
</body>

</html>