<!DOCTYPE html>
<html>
<head>
  <title>List Mahasiswa</title>
</head>
<body>
  <h1>List Mahasiswa</h1>
  <ul>
    <% mahasiswas.forEach(mahasiswa => { %>
      <li>
        <a href="/profile?id=<%= mahasiswa.id %>">
          <%= mahasiswa.name %> (NIM: <%= mahasiswa.nim %>)
        </a>
      </li>
    <% }); %>
  </ul>
</body>
</html>