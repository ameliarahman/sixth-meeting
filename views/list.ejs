<!DOCTYPE html>
<html>
<head>
  <title>List Mahasiswa</title>
</head>
<body>
  <h1>List Mahasiswa</h1>
  <ul>
    <% mahasiswaData.forEach(mahasiswa => { %>
      <li>
        <p>NAMA : <%= mahasiswa.name %></p>
        <p>NIM : <%= mahasiswa.nim %></p>
        <p>ALAMAT : <%= mahasiswa.address %></p>
      </li>
    <% }); %>
  </ul>
</body>
</html>