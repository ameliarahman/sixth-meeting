<!DOCTYPE html>
<html>
<head>
    <title>List Mahasiswa</title>
</head>
<body>
    <h1>List Mahasiswa</h1>
    <ul>
        <% mahasiswas.forEach(function(mahasiswa) { %>
            <li>
                <p>Nama: <%= mahasiswa.nama %></p>
                <p>NIM: <%= mahasiswa.nim %></p>
            </li>
        <% }) %>
    </ul>
</body>
</html>
